{
  // Version of the setting file.  Always 0.2
  "version": "0.2",
  // language - current active spelling language
  "language": "en,ru",
  // words - list of words to be always considered correct
  "words": [
    "ahlcg",
    "Arkham",
    "arktype",
    "arnopro",
    "bazel",
    "Conkordia",
    "doesnt",
    "gsap",
    "jsoneditor",
    "lintstagedrc",
    "ngrx",
    "panzoom",
    "prefersdark",
    "rombolshak",
    "twoarcane",
    "yoyo"
  ],
  // flagWords - list of words to be always considered incorrect
  // This is useful for offensive words and common spelling errors.
  // For example "hte" should be "the"
  "flagWords": [],
  "caseSensitive": false,
  "useGitignore": true,
  "dictionaries": [
    "bash",
    "companies",
    "cpp",
    "csharp",
    "css",
    "filetypes",
    "fonts",
    "go",
    "html",
    "latex",
    "misc",
    "node",
    "npm",
    "php",
    "powershell",
    "python",
    "softwareTerms",
    "typescript"
  ],
  "ignorePaths": ["*test*", "assets"],
  "import": ["@cspell/dict-ru_ru/cspell-ext.json"]
}
